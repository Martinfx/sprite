#! /bin/csh -f

foreach n (1 2 3 4 5)
    echo in...
    ds3100.md/copyin -iter 1000
    echo out...
    ds3100.md/copyin -iter 1000 -out
    echo in offset...
    ds3100.md/copyin -iter 1000 -offset
    echo out offset...
    ds3100.md/copyin -iter 1000 -out -offset
    echo in inband...
    ds3100.md/copyin -iter 1000 -inband
    echo out inband...
    ds3100.md/copyin -iter 1000 -out -inband
    echo in offset inband...
    ds3100.md/copyin -iter 1000 -offset -inband
    echo out offset inband...
    ds3100.md/copyin -iter 1000 -out -offset -inband
end
