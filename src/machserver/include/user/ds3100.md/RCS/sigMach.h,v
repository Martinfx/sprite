#ifndef _SIGMACHUSER
#define _SIGMACHUSER

/*
 * The different machine dependent codes for an arithmetic fault.
 */
#define SIG_OVERFLOW	1

/*
 * The different machine dependent codes for an illegal instruction signal
 */
#define SIG_FP_EXCEPTION	4

#endif /* _SIGMACHUSER */
@


