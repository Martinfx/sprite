# tests for "deleteuser prova":
end-of-line: adnan@@zion, downey@@cory, srinivas@@tycho, joshi@@arpa, prova,
	foo, bar
middle-of-line: foo, prova, bar
last-in-list: foo, bar, prova
only-on-line: adnan@@zion, downey@@cory, srinivas@@tycho, joshi@@arpa, konig@@venu,
	prova,
	doof
only-on-line-2: adnan@@zion, downey@@cory, srinivas@@tycho, joshi@@arpa, 
	konig@@venu,
	prova
only-in-list: prova
twice: foo, prova, bar, prova
thrice: foo, prova, prova, bar, prova
prova: prova@@foobarbaz
