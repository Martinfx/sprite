#if defined(LIBC_SCCS) && !defined(lint)
static char sccsid[] = "@@(#)memchr.c	5.3 (Berkeley) 5/25/88";
#endif /* LIBC_SCCS and not lint */

#include <string.h>

char *
memchr(s, c, n)
	register char *s;
	register c, n;
{
	while (--n >= 0)
		if (*s++ == c)
			return (--s);
	return (0);
}
@


