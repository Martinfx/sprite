#ifndef lint
static char rcsid[] = "$Header: /sprite/src/lib/c/stdlib/RCS/abs.c,v 1.1 88/05/21 12:14:46 ouster Exp Locker: rab $ SPRITE (Berkeley)";
#endif /* not lint */

#include <stdlib.h>

/*
 *----------------------------------------------------------------------
 *
 * abs --
 *
 *	Compute the absolute value of an integer.
 *
 * Results:
 *	The return value is j, unless j is negative, in which case
 *	the return value is -j.
 *
 * Side effects:
 *	None.
 *
 *----------------------------------------------------------------------
 */

int
abs(j)
    int j;			/* Value to take the absolute value of. */
{
    if (j >= 0) {
	return j;
    }
    return -j;
}
@


