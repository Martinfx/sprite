/*
 *  Pmax raw key code for the F13 key on the PMAX keyboard.
 *  This key is interpreted specially by DevKbdQueueAddInput().
 */

unsigned int devKbdSpecialKey = 0x73; 

/*
 * The raw key code for the break signal is the null character.
 */
unsigned int devKbdBreakKey = 0;

