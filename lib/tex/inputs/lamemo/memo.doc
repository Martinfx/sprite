      \put (\@countleftcolon,-162){\makebox(0,0)[br]{THRU:}}%
      \put (\@countleftcolon,-210){\makebox(0,0)[br]{FROM:}}%
      \put (\@countleftcolon,-234){\makebox(0,0)[br]{SUBJECT:}}%
