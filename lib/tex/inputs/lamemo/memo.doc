%    \put (0,0){\line(2,-1){40}}% debug corner
    \ifx\@headerfonts\@postscript
      \@dimentemp=\@dimenmemopaperhcorr
      \ifnum\@countthru=0\else\advance\@dimentemp by 3pt\fi
      \@counttemp=\@dimentemp
      \divide\@counttemp by 65536
      \@counttemptwo=\@dimenmemopapervcorr
