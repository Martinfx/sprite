# and cause a string of the form "<mkmf-script> <makeRCS?> <makeDepend?>" to
# be printed.  The first matching pattern wins, so order is important here.
d16 18
a33 18
/src\/lib\/include/			{ print "hdrs yes no"; exit }
/\.md$/					{ print "md no no"; exit }
/\/test\//				{ print "test yes yes"; exit }
/\/thymeC\/sprite\/src\/cmds\/[^/]*$/	{ print "command2 yes no"; exit }
/\/a\/att\/src\/cmds\/[^/]*$/		{ print "command2 yes no"; exit }
/\/a\/att\/src\/cmds$/			{ print "top no no"; exit }
/\/src\/kernel\/[^/]*$/			{ print "kernel yes no"; exit }
/\/a\/kernel\/[^/]*$/			{ print "kernel yes no"; exit }
/src\/lib\/cmds$/			{ print "cmds no no"; exit }
/src\/lib\/cmds\//			{ print "command yes yes"; exit }
/src\/lib\/[^/]*\/[^/]*$/		{ print "biglib yes no"; exit }
/src\/lib\/c$/				{ print "top no no"; exit }
/src\/lib\//				{ print "library yes no"; exit }
/src\/cmds$/				{ print "cmds no no"; exit }
/src\/daemons$/				{ print "cmds no no"; exit }
/src\/cmds\//				{ print "command yes yes"; exit }
/src\/daemons\//			{ print "command yes yes"; exit }
/src\/boot\//				{ print "boot yes yes"; exit }
